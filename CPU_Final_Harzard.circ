            regToPC     &lt;= '0';
        END IF;


        IF (opcode = "0010011") THEN -- R-Type com imediato
            AluSrc      &lt;= '1';
            blockA      &lt;= '0';
            RegWrite    &lt;= '1';
            MemRead     &lt;= '0';
            MemWrite    &lt;= '0';
