        IF (opcode = "1100111") THEN -- beq
            AluSrc      &lt;= '0';
            blockA      &lt;= '0';
            RegWrite    &lt;= '0';
            MemRead     &lt;= '0';
            MemWrite    &lt;= '0';
            Branch      &lt;= '1';
            AluOp       &lt;= "01";
            regSrc      &lt;= "00";
            BranchNotEq &lt;= '0';
            BrIncond    &lt;= '0';
