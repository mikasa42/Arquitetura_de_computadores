            regSrc      &lt;= "00";
            BranchNotEq &lt;= '1';
            BrIncond    &lt;= '0';
            regToPC     &lt;= '0';
        END IF;


        IF (opcode = "1100110") THEN -- jalr
            AluSrc      &lt;= '1';
            blockA      &lt;= '0';
            RegWrite    &lt;= '1';
            MemRead     &lt;= '0';
            MemWrite    &lt;= '0';
            Branch      &lt;= '0';
            AluOp       &lt;= "00";
            regSrc      &lt;= "10";
            BranchNotEq &lt;= '0';
            BrIncond    &lt;= '1';
            regToPC     &lt;= '1';
        END IF;


        IF (opcode = "1101111") THEN -- jal
            AluSrc      &lt;= '1';
            blockA      &lt;= '0';
            RegWrite    &lt;= '1';
            MemRead     &lt;= '0';
            MemWrite    &lt;= '0';
            Branch      &lt;= '0';
            AluOp       &lt;= "00";
            regSrc      &lt;= "10";
            BranchNotEq &lt;= '0';
            BrIncond    &lt;= '1';
            regToPC     &lt;= '0';
        END IF;


        IF (opcode = "0000011") THEN -- lw
            AluSrc      &lt;= '1';
            blockA      &lt;= '0';
            RegWrite    &lt;= '1';
            MemRead     &lt;= '1';
            MemWrite    &lt;= '0';
            Branch      &lt;= '0';
            AluOp       &lt;= "00";
            regSrc      &lt;= "01";
            BranchNotEq &lt;= '0';
            BrIncond    &lt;= '0';
            regToPC     &lt;= '0';
        END IF;


        IF (opcode = "0110111") THEN -- lui
            AluSrc      &lt;= '1';
            blockA      &lt;= '1';
            RegWrite    &lt;= '1';
            MemRead     &lt;= '0';
            MemWrite    &lt;= '0';
